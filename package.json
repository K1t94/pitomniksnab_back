    "apollo-server-errors": "^2.4.2",
    "apollo-server-express": "~2.16.1",
    "aws-sdk": "~2.566.0",
    "babel-eslint": "~10.0.3",
    "babel-plugin-relay": "~3.0.0",
    "body-parser": "~1.18.3",
    "compression": "~1.7.4",
    "cors": "~2.8.5",
    "dotenv": "~8.2.0",
    "dotenv-expand": "~5.1.0",
    "express": "~4.17.1",
    "graphql": "~14.5.8",
    "graphql-subscriptions": "^1.1.0",
    "graphql-upload": "~8.1.0",
    "http-proxy-middleware": "~0.20.0",
