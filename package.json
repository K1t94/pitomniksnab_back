    "typescript": "~3.4.1",
    "uuid": "~3.3.3"
  },
  "scripts": {
    "start": "NODE_ENV=development yarn nodemon --config nodemon-dev.json --exec ts-node --project tsconfig.json ./src/server/index.ts",
    "serve-prod": "NODE_ENV=production ts-node --project tsconfig.json ./src/server/index.ts",
    "serve:debug": "nodemon --config nodemon-debug.json",
    "migrations:create": "yarn typeorm migration:create",
    "migrations:diff": "yarn typeorm migration:generate",
    "migrations:migrate": "yarn typeorm migration:run",
    "migrations:revert": "yarn typeorm migration:revert",
    "typeorm": "ts-node --project tsconfig.json ./node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~2.7.0",
    "eslint": "~6.6.0",
    "eslint-config-airbnb-base": "~14.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "tslint": "~5.20.1",
    "tslint-eslint-rules": "~5.4.0"
  }
